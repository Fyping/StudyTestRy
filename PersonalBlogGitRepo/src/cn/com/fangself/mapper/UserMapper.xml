<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.fangself.mapper.UserMapper">

  <select id="countUipFromUserrTable" parameterType="java.lang.String" resultType="java.lang.Integer">
  		select count(*) from tb_User where uip=#{uip}; 
  </select>
  
  <insert id="insertUipIntoUserTable" parameterType="java.lang.String">
  	insert into tb_User (uip) values (#{uip});
  </insert>
</mapper>